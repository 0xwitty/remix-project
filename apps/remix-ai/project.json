{
  "name": "remix-ai",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/remix-ai/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "development",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/remix-ai",
        "index": "apps/remix-ai/src/index.html",
        "baseHref": "./",
        "main": "apps/remix-ai/src/main.tsx",
        "polyfills": "apps/remix-ai/src/polyfills.ts",
        "tsConfig": "apps/remix-ai/tsconfig.app.json",
        "assets": [
          "apps/remix-ai/src/favicon.ico",
          "apps/remix-ai/src/profile.json"
        ],
        "styles": ["apps/remix-ai/src/styles.css"],
        "scripts": [],
        "webpackConfig": "apps/remix-ai/webpack.config.js"
      },
      "configurations": {
        "development": {
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/remix-ai/src/environments/environment.ts",
              "with": "apps/remix-ai/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/remix-ai/**/*.ts"],
        "eslintConfig": "apps/remix-ai/.eslintrc"
      }
    },
    "serve": {
      "executor": "@nrwl/webpack:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "remixai:build",
        "hmr": true,
        "baseHref": "/"
      },
      "configurations": {
        "development": {
          "buildTarget": "remixai:build:development",
          "port": 5002
        },
        "production": {
          "buildTarget": "remixai:build:production"
        }
      }
    }
  },
  "tags": []
}
